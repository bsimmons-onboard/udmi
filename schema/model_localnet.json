{
  "$udmi_version": "1.4.0",
  "title": "Localnet Model",
  "description": "Used to describe device local network parameters",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "networks": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, NetworkLocalnetModel>",
      "patternProperties": {
        "^[a-z0-9-]+$": {
          "$ref": "file:model_localnet_network.json"
        }
      }
    }
  },
  "required": [
    "networks"
  ]
}
